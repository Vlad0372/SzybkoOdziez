<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SzybkoOdziez.Views.ItemCarouselPage">
    <ContentPage.Content>
        <CarouselView ItemsSource="{Binding imageSources}" >
            <CarouselView.ItemTemplate>
                <DataTemplate >
                    <StackLayout  Style="{StaticResource SZO_StyleBG2}">
                        
                            <StackLayout>
                               
                                <Image Source="{Binding}"
                                       HeightRequest="600"
                                       WidthRequest="600"/>
                            </StackLayout>
                        
                    </StackLayout>
                </DataTemplate>
            </CarouselView.ItemTemplate>
        </CarouselView>
    </ContentPage.Content>
</ContentPage>